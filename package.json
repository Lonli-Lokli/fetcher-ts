{
  "name": "@lonli-lokli/fetcher-ts",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "lint": "nx lint",
    "test": "nx test",
    "build": "run-s build:js build:copy build:check",
    "build:copy": "shx cp ./packages/fetcher-typebox/package.json ./build/package.json && shx cp ./README.md ./build/README.md",
    "build:js": "tsup",
    "build:check": "attw --pack build",
    "typecheck": "nx typecheck",
    "release": "nx release --skip-publish"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@eslint/js": "^9.8.0",
    "@sinclair/typebox": "0.34.27",
    "cross-fetch": "^4.0.0",
    "@nx/eslint": "20.4.6",
    "@nx/eslint-plugin": "20.4.6",
    "@nx/js": "20.4.6",
    "@nx/vite": "20.4.6",
    "@nx/web": "20.4.6",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "18.16.9",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jsdom": "~22.1.0",
    "nx": "20.4.6",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "verdaccio": "^5.0.4",
    "shx": "0.3.4",
    "tsup": "^8.2.4",
    "vite": "^5.0.0",
    "vite-plugin-dts": "~3.8.1",
    "vitest": "^1.3.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
